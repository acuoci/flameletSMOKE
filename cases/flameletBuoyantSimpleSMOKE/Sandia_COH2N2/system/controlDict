FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       flameletSimpleSMOKE;

startFrom         latestTime;

startTime         0;

stopAt            endTime;

endTime           30000;

deltaT            1;

writeControl      timeStep;

writeInterval     1000;

purgeWrite        0;

writeFormat       ascii;

writePrecision    9;

writeCompression  uncompressed;

timeFormat        general;

timePrecision     6;

runTimeModifiable yes;

